local ErrorPrompt = getrenv().require(game.CoreGui.RobloxGui.Modules.ErrorPrompt)
local prompt = ErrorPrompt.new("Default")
prompt._hideErrorCode = true
local gui = Instance.new("ScreenGui", game.CoreGui)
prompt:setParent(gui)
prompt:setErrorTitle("are u sure about that?")
prompt:updateButtons(
    {
        {
            Text = "Close Script",
            Callback = function()
                prompt:_close()
                game:GetService("RunService"):SetRobloxGuiFocused(false)
                sethiddenproperty(gui, "OnTopOfCoreBlur", false)
            end,
            Primary = false
        },
        {
            Text = "Load Script",
            Callback = function()
                prompt:_close()
                game:GetService("RunService"):SetRobloxGuiFocused(false)
                sethiddenproperty(gui, "OnTopOfCoreBlur", false)
		loadstring(game:HttpGet("https://pastebin.com/raw/3TGVeRkH",true))()
            end,
            Primary = true
        }
    },
    "Default"
)
prompt:_open(" want load ESKID? ")

game:GetService("RunService"):SetRobloxGuiFocused(true)
sethiddenproperty(gui, "OnTopOfCoreBlur", true)
